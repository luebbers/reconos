#uses "xillib.tcl"

proc generate {drv_handle} {
  set level [xget_value $drv_handle "PARAMETER" "level"]
  if {$level == 0} {
    xdefine_include_file $drv_handle "xparameters.h" "XTft" "NUM_INSTANCES" "C_DCR_BASEADDR" "C_DCR_HIGHADDR" "C_DEFAULT_TFT_BASE_ADDR"
  }
  if {$level == 1} {
    set dname [xget_dname "XTft" "NUM_INSTANCES"]
    xdefine_include_file $drv_handle "xparameters.h" "XTft" "NUM_INSTANCES" "C_DCR_BASEADDR" "C_DCR_HIGHADDR" "DEVICE_ID" "C_DEFAULT_TFT_BASE_ADDR"
    xdefine_config_file $drv_handle "xtft_g.c" "XTft" "DEVICE_ID" "C_DEFAULT_TFT_BASE_ADDR"
  }
}
